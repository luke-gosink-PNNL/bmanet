	Keywords = {electrostatics; enhanced{\_}sampling; molecular{\_}dynamics; {p$K_a$}; solvation; titration; *bma-2013},
	Number = {12},
	Pages = {3364--3373},
	Publisher = {Wiley Subscription Services, Inc., A Wiley Company},
	Title = {Toward accurate prediction of {p$K_a$} values for internal protein residues: The importance of conformational relaxation and desolvation energy},
	Ty = {JOUR},
