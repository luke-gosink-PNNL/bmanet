	Keywords = {electrostatics; {p$K_a$}; solvation; structure},
	Number = {13},
	Pages = {5260--5265},
	Publisher = {National Academy of Sciences},
	Title = {Large shifts in {p$K_a$} values of lysine residues buried inside a protein},
	Ty = {JOUR},
