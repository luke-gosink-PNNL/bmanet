	Journal = {Journal of Molecular Biology},
	Keywords = {electrostatics; pka; protein{\_}folding; solvation},
	Number = {2},
	Pages = {361--377},
	Title = {Structural Origins of High Apparent Dielectric Constants Experienced by Ionizable Groups in the Hydrophobic Core of a Protein},
	Ty = {JOUR},
	Url = {http://dx.doi.org/10.1016/j.jmb.2010.10.001},
