	Number = {12},
	Pages = {3276--3286},
	Publisher = {Wiley Subscription Services, Inc., A Wiley Company},
	Title = {Predicting extreme {p$K_a$} shifts in staphylococcal nuclease mutants with constant {pH} molecular dynamics.},
