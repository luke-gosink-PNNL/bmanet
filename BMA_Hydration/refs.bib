	Number = {12},
	Pages = {3306-3319},
	Publisher = {Wiley Subscription Services, Inc., A Wiley Company},
	Title = {{MCCE} analysis of the {p$K_a$s} of introduced buried acids and bases in staphylococcal nuclease.},
