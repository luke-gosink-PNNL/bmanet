	Isbn = {1089-8638},
	Journal = {Journal of Molecular Biology},
	Keywords = {electrostatics; pka; solvation; titration},
	Number = {1},
	Pages = {34--47},
	Title = {The {p$K_a$} values of acidic and basic residues buried at the same internal location in a protein are governed by different factors.},
	Ty = {JOUR},
