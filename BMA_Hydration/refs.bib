	Isbn = {03014622},
	Journal = {Biophysical Chemistry},
	Keywords = {electrostatics; pka; protein{\_}engineering; protein{\_}folding},
	Title = {Thermodynamic principles for the engineering of {pH}-driven conformational switches and acid insensitive proteins},
	Ty = {JOUR},
