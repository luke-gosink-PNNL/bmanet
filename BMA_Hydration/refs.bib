	Keywords = {electrostatics; nmr; pka; solvation; titration},
	Number = {5},
	Pages = {833-845},
	Title = {A buried lysine that titrates with a normal {p$K_a$}: role of conformational flexibility at the protein-water interface as a determinant of {p$K_a$} values.},
	Ty = {JOUR},
	Url = {http://dx.doi.org/10.1110/ps.073397708},
