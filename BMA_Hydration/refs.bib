	Keywords = {electrostatics; {p$K_a$}; solvation; titration; *bma-2013},
	Number = {12},
	Pages = {3374--3380},
	Publisher = {Wiley Subscription Services, Inc., A Wiley Company},
	Title = {{p$K_a$} predictions with a coupled finite difference {Poisson}-{Boltzmann} and {Debye}-{H{\"u}ckel} method.},
