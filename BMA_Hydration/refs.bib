	Id = {citeulike:7275811},
	Isbn = {1520-6106},
	Journal = {The Journal of Physical Chemistry B},
	Keywords = {molecular{\_}simulation; pka; quantum{\_}mechanics},
	Number = {28},
	Pages = {8387-8397},
	Title = {{p$K_a$} of Residue 66 in Staphylococal nuclease. {I.} Insights from QM/MM Simulations with Conventional Sampling},
	Ty = {JOUR},
	Url = {http://dx.doi.org/10.1021/jp800168z},
	Volume = {112},
