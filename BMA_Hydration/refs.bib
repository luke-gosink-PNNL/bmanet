	Isbn = {1097-0134},
	Journal = {Proteins},
	Keywords = {electrostatics; {p$K_a$}; solvation; titration; *bma-2013},
	Number = {12},
	Pages = {3320--3332},
	Publisher = {Wiley Subscription Services, Inc., A Wiley Company},
	Title = {Electrostatic {p$K_a$} computations in proteins: role of internal cavities.},
	Ty = {JOUR},
	Url = {http://dx.doi.org/10.1002/prot.23092},
